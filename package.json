{
  "name": "@handy-common-utils/dev-utils",
  "version": "1.2.6",
  "description": "Utilities for the convenience of developers",
  "main": "dist/dev-utils.js",
  "types": "dist/dev-utils.d.ts",
  "bin": {
    "generate-api-docs-and-update-readme": "dist/bin/generate-api-docs-and-update-readme.js"
  },
  "scripts": {
    "pretest": "eslint . --ext .ts",
    "test": "nyc mocha",
    "fixPeerDependencies": "shx sed -i 's/^ +\"serverless\": *\".+\",*$//' node_modules/serverless-plugin-git-variables/package.json; shx sed -i 's/^ +\"serverless\": *\".+\",*$//' package-lock.json",
    "prepare": "shx rm -rf dist && npm run fixPeerDependencies && tsc && shx chmod +x dist/bin/*.js && es-check",
    "preversion": "npm run prepare && dist/bin/generate-api-docs-and-update-readme.js && git add README.md"
  },
  "files": [
    "package.json",
    ".eslintignore",
    ".eslintrc.*",
    ".nycrc.*",
    "tsconfig.*",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/handy-common-utils/dev-utils.git"
  },
  "keywords": [
    "handy-common-utils",
    "dev",
    "utils"
  ],
  "author": "James Hu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/handy-common-utils/dev-utils/issues"
  },
  "homepage": "https://github.com/handy-common-utils/dev-utils#readme",
  "dependencies": {
    "@handy-common-utils/fs-utils": "^1.0.15",
    "concat-md": "^0.3.8",
    "lodash": "^4.17.21",
    "serverless-plugin-git-variables": "^5.2.0",
    "string.prototype.replaceall": "^1.0.6",
    "typedoc": "^0.22.12",
    "typedoc-plugin-markdown": "^3.11.14",
    "yaml": "^2.0.1"
  },
  "bundledDependencies": [
    "serverless-plugin-git-variables"
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.1",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "es-check": "^6.2.1",
    "eslint": "^7.25.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "shx": "^0.3.4",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "volta": {
    "node": "16.17.1",
    "npm": "8.19.2"
  }
}
