  },
  "repository": {
    "type": "git",
    "url": "https://github.com/handy-common-utils/dev-utils.git"
  },
  "keywords": [
    "handy-common-utils",
    "dev",
    "utils"
  ],
  "author": "James Hu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/handy-common-utils/dev-utils/issues"
  },
  "homepage": "https://github.com/handy-common-utils/dev-utils#readme",
  "dependencies": {
    "@handy-common-utils/fs-utils": "^1.0.8",
    "concat-md": "^0.3.5",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.0.11"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.2",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "mocha": "^8.2.0",
    "shx": "^0.3.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
